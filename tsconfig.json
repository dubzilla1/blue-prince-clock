{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./out-tsc",
    "module": "es2020",
    "target": "es2020",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "declaration": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "angularCompilerOptions": {
    "enableIvy": true
  }
}
